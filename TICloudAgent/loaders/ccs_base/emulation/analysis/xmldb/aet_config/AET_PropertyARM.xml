	  <KEYITEM>
		  <KEYNAME>TRACE DATA FOR ALL</KEYNAME>	
		  <KEYNUMBER>1</KEYNUMBER>	
	  </KEYITEM>
  	  <VALUE>1</VALUE>
	</PROPERTY_BOOL>

	<PROPERTY_BOOL>
	  <NAME>Data</NAME>	
 	  <DESCRIPTION>Include Data Bus</DESCRIPTION>		
	  <KEYITEM>
		  <KEYNAME>TRACE DATA FOR ALL</KEYNAME>	
		  <KEYNUMBER>2</KEYNUMBER>	
	  </KEYITEM>
	  <KEYITEM>
		  <KEYNAME>TRACE DATA ONLY</KEYNAME>	
		  <KEYNUMBER>2</KEYNUMBER>	
	  </KEYITEM>
	  <EDITABLE>FALSE</EDITABLE>
  	  <VALUE>1</VALUE>
	</PROPERTY_BOOL>

